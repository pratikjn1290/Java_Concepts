package otherPrograms;

public class ArmstrongNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//Armstrong number means qube of number is same as number
		
		int number = 153;
		int temp;
		int sum = 0;
		
		while(number>0)
		{
			int rev = number % 10;
			temp = sum + (rev * rev * rev);
		}

	}

}

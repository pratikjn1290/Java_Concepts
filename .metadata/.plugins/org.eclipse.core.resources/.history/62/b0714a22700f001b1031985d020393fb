package otherPrograms;

public class PelindromNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int number = 151;
		int rev = 0;
		
		while(number != 0)
		{
			int digit = number % 10;
			rev = rev + (digit * 10);
			number = number/10;
			System.out.println(rev);
		}
			
		
		
	}

}

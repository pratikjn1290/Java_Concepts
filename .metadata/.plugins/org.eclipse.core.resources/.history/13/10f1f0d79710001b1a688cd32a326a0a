package stringfunctions;

public class SwapString {

	public static void main(String[] args) {

		String s = "Hello";
		String s1 = "How";

		String s = s + s1;
		s1 = s.substring(0, s.length() - s2.length());
		s1 = s.substring(s2.length());
		
		System.out.println(s1);
		System.out.println(s2);
	}

}

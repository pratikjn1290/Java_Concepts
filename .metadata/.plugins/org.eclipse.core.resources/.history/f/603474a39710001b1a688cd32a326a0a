package stringfunctions;

public class SwapString {

	public static void main(String[] args) {

		String s1 = "Hello";
		String s2 = "How";

		String s = s1 + s2;
		s2 = s.substring(0, s.length() - s1.length());
		s2 = s.substring(s2.length());
		
		System.out.println(s1);
		System.out.println(s2);
	}

}

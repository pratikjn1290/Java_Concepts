package string;

public class IsNumeric {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String s1 = "String";
		
		String regex = "[a-z A-Z]";
		
		int len = s1.length();
		
		if(!s1.contains(regex))
		{
			System.out.println("Have Digits");
		}
		
		else
		{
			System.out.println("No digits");
		}
		

	}

}


public class FibonaciTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		class fibonaci{

			public static void main(String args[])

			int fact;

			int temp = 1;

			for(int i = 0; i<1000; i++)

			{

			fact = temp;

			fact = fact+ i;

			temp = fact;

			System.Out.println(temp)

			}

			}
		
		
		
	}

}

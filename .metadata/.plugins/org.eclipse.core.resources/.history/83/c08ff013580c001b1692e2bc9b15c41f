package string;

public class Repeatedchar {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String s = "Hello How are you! Hello I am fine";
		
		int len = s.length();
		int count = 0;
		for(int i = 0 ; i <=len; i++)
		{
			for (int j = 1; j<=len; j++)
				if(i = j && s[i] == s[j])
		}
		
		
		
		
	}

}
